{
  "version": "5.9.0",
  "integrations": [
    "agents-md",
    "claude-code"
  ],
  "templates": {
    "prdQuestions": "default",
    "fullPrd": "default",
    "quickPrd": "default"
  },
  "outputs": {
    "path": ".clavix/outputs",
    "format": "markdown"
  },
  "tasks": {
    "paths": [
      "docs/features/*-tasks.md"
    ],
    "defaultFile": "docs/features/core-gameplay-tasks.md"
  },
  "preferences": {
    "autoOpenOutputs": false,
    "verboseLogging": false
  }
}